{
  "cli": {
    "version": ">= 0.57.0"
  },
  "build": {
    "amazon-production": {
      "android": {
        "credentialsSource": "local"
      }
    },
    "google-production": {
      "android": {
        "credentialsSource": "local"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "channel": "dev"
      },
      "ios": {
        "channel": "dev-ios",
        "resourceClass": "m1-medium",
        "simulator": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "staging",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "resourceClass": "m1-medium"
      }
    },
    "production": {
      "android": {
        "channel": "production-fcm"
      },
      "ios": {
        "channel": "production-ios-fcm",
        "resourceClass": "m1-medium"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "daclenmobile@gmail.com"
      }
    }
  }
}
